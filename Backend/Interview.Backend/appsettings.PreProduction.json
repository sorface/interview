{
  "Logging": {
    "LogLevel": {
      "Interview": "Debug",
      "Microsoft.AspNetCore": "Error",
      "AspNet": "Error",
      "Microsoft": "Error"
    }
  },
  "CorsOptions" : {
    "AllowedOrigins": [
      "https://*.sorface.ru",
      "https://api.idp.sorface.ru",
      "https://api.gateway.sorface.ru",
      "https://interview.sorface.ru",
      "https://sorface.ru"
    ]
  },
  "ConnectionStrings": {
    "database": "Host=localhost;Port=5444;Username=user;Password=user;Database=interview",
    "type": "postgres"
  },
  "OpenIdConnectOptions": {
    "Issuer": "https://api.idp.sorface.ru",
    "MetadataPath": "/.well-known/openid-configuration"
  },
  "SwaggerOption": {
    "RoutePrefix": "api"
  }
}
